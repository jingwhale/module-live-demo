{
    "extends": "eslint:recommended",
    "rules": {
        "no-control-regex":[
            "warn"
        ],
        "no-else-return": [
            "warn"
        ],
        "curly": [
            "warn",
            "all"
        ],
        "no-empty-function": [
            "warn"
        ],
        "no-magic-numbers": [
            "warn"
        ],
        "no-unmodified-loop-condition": [
            "warn"
        ],
        "vars-on-top": [
            "warn"
        ],
        "no-use-before-define": [
            "error"
        ],
        "max-len": [
            "warn",
            150
        ],
        "no-spaced-func": [
            "warn"
        ],
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error"
                ]
            }
        ]
    },
    "globals": {
        "jQuery": true,
        "$": true,
        "NEJ": true,
        "define": true,
        "window": true,
        "Regular": true,
        "console": true
    },
    "fix": true
}
