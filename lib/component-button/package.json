{
    "name": "component-button",
    "version": "0.0.37",
    "description": "component button project",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "npm prune && npm install --registry=https://registry.npm.taobao.org && bower prune && bower update",
        "test": "gulp coverage && mocha-phantomjs test/coverage.html --hooks test/hook.js",
        "lint": "eslint src/**/*.js",
        "css": "sass -I ./lib --watch --scss --no-cache --unix-newlines src:src -t compressed",
        "jsdoc": "jsdoc -c .jsdoc.json",
        "coverage-report": "./node_modules/.bin/istanbul report --root test/coverage --dir test/coverage lcov",
        "coverage-cmd-report": "./node_modules/.bin/istanbul report --root test/coverage --dir test/coverage text-summary"
    },
    "repository": {
        "type": "git",
        "url": "ssh://git@g.hz.netease.com:22222/edu-frontend/component-button.git"
    },
    "keywords": [
        "edu",
        "component",
        "button"
    ],
    "author": "hzshaoyy",
    "license": "MIT",
    "devDependencies": {
        "gulp": "^3.9.1",
        "replacestream": "^4.0.2",
        "ink-docstrap": "^1.2.1",
        "istanbul": "^0.4.5",
        "mocha-phantomjs": "^4.1.0",
        "gulp-istanbul": "^1.1.1"
    }
}