{
    "name": "edu-front-util",
    "version": "0.3.30",
    "description": "utils for NetEase Education FrontEnd Group",
    "main": "index.js",
    "scripts": {
        "docs": "jsdoc2md src/*.js > api.md",
        "coverage-report": "./node_modules/.bin/istanbul report --root coverage lcov",
        "coverage-cmd-report": "./node_modules/.bin/istanbul report --root coverage text-summary",
        "test": "gulp test && mocha-phantomjs test/coverage.html --hooks test/hook.js",
        "test_syy": "gulp test && mocha-phantomjs -p /usr/local/bin/phantomjs test/coverage.html --hooks test/hook.js",
        "js2doc": "jsdoc -c .jsdoc.json",
        "lint": "eslint src/**/*.js --quiet"
    },
    "author": "hzshaoyy",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "chai": "^3.5.0",
        "gulp": "^3.9.1",
        "gulp-istanbul": "^1.1.1",
        "istanbul": "^0.4.5",
        "jsdoc-to-markdown": "^1.3.3",
        "mocha": "^2.3.4",
        "mochawesome": "^1.3.4",
        "ink-docstrap": "^1.2.1"
    }
}