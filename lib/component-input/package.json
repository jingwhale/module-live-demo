{
    "name": "component-input",
    "version": "0.0.49",
    "description": "component project template",
    "main": "src/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "npm prune && npm install --registry=https://registry.npm.taobao.org && bower prune && bower update",
        "lint": "eslint src/**/*.js",
        "css": "sass --watch --scss --no-cache --unix-newlines src:src -t compressed",
        "jsdoc": "jsdoc -c conf.json",
        "coverage-report": "./node_modules/.bin/istanbul report --root test/coverage --dir test/coverage lcov",
        "coverage-cmd-report": "./node_modules/.bin/istanbul report --root test/coverage --dir test/coverage text-summary",
        "test": "gulp coverage && mocha-phantomjs test/coverage.html --hooks test/hook.js",
        "test_syy": "gulp coverage && mocha-phantomjs -p /usr/local/bin/phantomjs test/coverage.html --hooks test/hook.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://g.hz.netease.com/edu-frontend/component-input.git"
    },
    "keywords": [
        "edu",
        "component"
    ],
    "author": "edu",
    "license": "MIT",
    "devDependencies": {
        "gulp": "^3.9.1",
        "replacestream": "^4.0.2",
        "ink-docstrap": "^1.2.1",
        "gulp-istanbul": "^1.1.1",
        "istanbul": "^0.4.5"
    }
}